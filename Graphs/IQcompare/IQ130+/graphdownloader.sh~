#!/bin/bash

s1='http://openconnecto.me/mrdata/share/dti/ndmg_v0011/MRN114/DS00071/MRN114_'
s2='_1_DTI_DS00071.graphml'

for i in 'M87156106' 'M87192637' 'M87146520' 'M87126413' 'M87191087' 'M87199728' 'M87121943' 'M87134068' 'M87146993' 'M87185000' 'M87151146' 'M87103074' 'M87148745' 'M87188000' 'M87196363' 'M87129719' 'M87136332' 'M87144896' 'M87155496' 'M87158534' 'M87183189' 'M87183485' 'M87108094' 'M87141664' 'M87142764' 'M87191258' 'M87190609' 'M87165441' 'M87128444' 'M87157827' 'M87166115' 'M87181205' 'M87187984'
do
	c=$s1$i$s2
	wget $c

done
